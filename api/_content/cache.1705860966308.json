or all collection and processing materials."}]}]},{"type":"element","tag":"template","props":{"v-slot:actions":""},"children":[{"type":"element","tag":"p","props":{},"children":[{"type":"element","tag":"a","props":{"href":"/assets/docs/biospecimen-data/bal/Verberk_2021_AlzDem_Pre-analytics.pdf","target":"_blank","className":["rvt-cta","rvt-m-bottom-sm","download"]},"children":[{"type":"text","value":"View sample collection standard"}]}]}]}]}]},{"type":"element","tag":"column","props":{"className":["rvt-flex"],"lg":"4","md":"6"},"children":[{"type":"element","tag":"card","props":{"title":"Sample Processing"},"children":[{"type":"element","tag":"template","props":{"v-slot:content":""},"children":[{"type":"element","tag":"p","props":{},"children":[{"type":"text","value":"For sample processing, please refer to the following schematics."}]}]},{"type":"element","tag":"template","props":{"v-slot:actions":""},"children":[{"type":"element","tag":"p","props":{},"children":[{"type":"element","tag":"a","props":{"href":"/assets/docs/biospecimen-data/bal/BAL_Schematics_-_Plasma__Buffy_Coat.pdf","target":"_blank","className":["rvt-cta","download","rvt-m-bottom-none"]},"children":[{"type":"text","value":"BAL Schematics-Plasma, Buffy Coat"}]},{"type":"element","tag":"a","props":{"href":"/assets/docs/biospecimen-data/bal/BAL_Schematics_-_Plasma.pdf","target":"_blank","className":["rvt-cta","download"]},"children":[{"type":"text","value":"BAL Schematics-Plasma"}]}]}]}]}]},{"type":"element","tag":"column","props":{"className":["rvt-flex"],"lg":"4","md":"6"},"children":[{"type":"element","tag":"card","props":{"title":"Shipping Information"},"children":[{"type":"element","tag":"template","props":{"v-slot:content":""},"children":[{"type":"element","tag":"p","props":{},"children":[{"type":"text","value":"Please ship frozen specimens Monday–Wednesday."}]},{"type":"element","tag":"p","props":{},"children":[{"type":"text","value":"Please organize the samples within their shipment box in the s