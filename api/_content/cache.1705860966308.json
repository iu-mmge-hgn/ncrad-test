hildren":[]},{"type":"text","value":"\n    "},{"type":"element","tag":"figcaption","props":{},"children":[{"type":"text","value":"ALLFTD"}]}]}]},{"type":"element","tag":"column","props":{"lg":"4","md":"6","sm":"12","xs":"12"},"children":[{"type":"element","tag":"picture","props":{},"children":[{"type":"text","value":"\n    "},{"type":"element","tag":"img","props":{"src":"/assets/studies/logos/psdc_logo_combo.png","alt":"Parkinson’s Spectrum Disorders Center","style":{"aspectRatio":"2002/1256"}},"children":[]},{"type":"text","value":"\n    "},{"type":"element","tag":"figcaption","props":{},"children":[{"type":"text","value":"Parkinson’s Spectrum Disorders Center"}]}]}]},{"type":"element","tag":"column","props":{"lg":"4","md":"6","sm":"12","xs":"12"},"children":[{"type":"element","tag":"picture","props":{},"children":[{"type":"text","value":"\n    "},{"type":"element","tag":"img","props":{"src":"/assets/studies/logos/DODadni.png","alt":"US Department of Defense – Alzheimer’s Disease Neuroimaging Initiative","style":{"aspectRatio":"318/205"}},"children":[]},{"type":"text","value":"\n    "},{"type":"element","tag":"figcaption","props":{},"children":[{"type":"text","value":"US Department of Defense – Alzheimer’s Disease Neuroimaging Initiative"}]}]}]},{"type":"element","tag":"column","props":{"lg":"4","md":"6","sm":"12","xs":"12"},"children":[{"type":"element","tag":"picture","props":{},"children":[{"type":"text","value":"\n    "},{"type":"element","tag":"img","props":{"src":"/assets/studies/logos/adni3.png","alt":"Alzheimer’s Disease Neuroimaging Initiative","style":{"aspectRatio":"400/277"}},"children":[]},{"type":"text","value":"\n    "},{"type":"element","tag":"figcaption","props":{},"children":[{"type":"text","value":"Alzheimer’s Disease Neuroimaging Initiative"}]}]}]},{"type":"element","tag":"column","props":{"lg":"4","md":"6","sm":"12","xs":"12"},"children":[{"type":"element","tag":"picture","props":{},"children":[{"type":"text","value":"\n    "},{"type":"element"